<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= content_for?(:title) ? yield(:title) : " Memoly" %></title>
    <meta name="description" content="">
    <meta name="author" content="">
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <%= yield(:head) %>
  </head>
  <body>
    <div class="container">
        <%= yield %>
    </div>
    <footer class="footer">
      <hr>
      <div class="container">
        <p>&copy;<%= Time.now.year %> MemolyApp. All rights reserved. </p>
      </div>
    </footer>
  </body>
</html>
